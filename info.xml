<?xml version="1.0"?>
<extension key="net.ourpowerbase.engage" type="module">
  <file>engage</file>
  <name>Engage</name>
  <description>Extra functionality for grassroots organizing</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Jamie McClelland</author>
    <email>jamie@progressivetech.org</email>
  </maintainer>
  <releaseDate>2015-02-02</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Engage</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Engage_Upgrader</upgrader>
</extension>
